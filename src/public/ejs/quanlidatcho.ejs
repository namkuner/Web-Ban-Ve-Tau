<script>
    function searchTicket() {
  var phoneNumber = document.getElementById('phone-number').value;

  // Ẩn bảng dữ liệu ban đầu
  document.getElementById('inforbooker').style.display = 'none';

  // Duyệt qua từng hàng trong bảng
  var rows = document.querySelectorAll('#inforbooker tbody tr');
  for (var i = 0; i < rows.length; i++) {
    var row = rows[i];
    var Phone = row.querySelector('td:nth-child(2)').innerText;

    // So sánh số điện thoại
    if (Phone === phoneNumber) {
      // Nếu trùng khớp, lấy thông tin và hiển thị
      var Hoten = row.querySelector('td:nth-child(1)').innerText;
      var Email = row.querySelector('td:nth-child(3)').innerText;
      var CCCD = row.querySelector('td:nth-child(4)').innerText;
      var Sove = row.querySelector('td:nth-child(5)').innerText;

      // Hiển thị thông tin
      document.getElementById('Hoten').textContent = Hoten;
      document.getElementById('Phone').textContent = Phone;
      document.getElementById('Email').textContent = Email;
      document.getElementById('CCCD').textContent = CCCD;
      document.getElementById('Sove').textContent = Sove;

      // Hiển thị kết quả và dừng vòng lặp sau khi tìm thấy
      document.getElementById('ticket-info').style.display = 'block';
      break;
    }
  }
}


    </script>

